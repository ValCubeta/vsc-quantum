{
	"name": "sunny",
	"displayName": "Sunny Syntax Highlighting",
	"description": "Syntax highlighting for the Sunny programming language",
	"version": "1.1.0",
	"icon": "images/sunny.png",
	"publisher": "valcubeto",
	"author": {
		"name": "Val Cubeto",
		"url": "https://github.com/valcubeto"
	},
	"engines": {
		"vscode": "^1.22.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Visualization",
		"Language Packs"
	],
	"keywords": [
		"sunny",
		"syntax",
		"highlight",
		"highlighting",
		"language",
		"grammar",
		"snippets",
		"code",
		"vscode"
	],
	"main": "./extension.js",
	"contributes": {
		"grammars": [
			{
				"language": "sunny",
				"scopeName": "source.sny",
				"path": "./sunny.tmLanguage.json"
			}
		],
		"languages": [
			{
				"id": "sunny",
				"aliases": [
					"Sunny",
					"sunny"
				],
				"extensions": [
					".sny"
				],
				"configuration": "./sunny-lang-config.json"
			}
		],
		"snippets": [
			{
				"language": "sunny",
				"path": "./sunny.code-snippets"
			}
		]
	},
	"license": "MIT",
	"bugs": "https://github.com/valcubeto/vsc-sunny/issues",
	"repository": "https://github.com/valcubeto/vsc-sunny",
	"__metadata": {
		"publisherDisplayName": "Val Cubeto",
		"size": 15922744
	}
}