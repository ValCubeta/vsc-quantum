{
	"name": "sunny",
	"displayName": "Sunny syntax",
	"description": "Syntax highlighting for the Sunny programming language",
	"version": "1.0.0",
	"publisher": "valcubeta",
	"author": {
		"name": "Val Cubeta",
		"url": "https://twitter.com/valcubetaa"
	},
	"engines": {
		"vscode": "^1.22.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"sunny syntax",
		"sunny highlight"
	],
	"main": "./extension.js",
	"contributes": {
		"grammars": [
			{
				"language": "sunny",
				"scopeName": "source.sny",
				"path": "./sunny.tmLanguage.json"
			}
		],
		"languages": [
			{
				"id": "sunny",
				"aliases": [
					"Sunny",
					"sunny"
				],
				"extensions": [
					".sny"
				],
				"configuration": "./sunny-lang-config.json"
			}
		],
		"snippets": [
			{
				"language": "sunny",
				"path": "./sunny.code-snippets"
			}
		]
	},
	"license": "MIT",
	"bugs": "https://github.com/ValCubeta/vsc-sunny/issues",
	"repository": "https://github.com/ValCubeta/vsc-sunny",
	"__metadata": {
		"publisherDisplayName": "Val Cubeta"
	}
}