{
	"name": "quantum",
	"displayName": "Quantum",
	"description": "Quantum syntax highlighting and snippets",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": ["Programming Languages"],
	"repository": "https://github.com/ValentineBucket/vsc-quantum",
	"activationEvents": ["onLanguage:quantum"],
	"main": "./extension.js",
	"contributes": {
		"languages": [{
			"id": "quantum",
			"aliases": ["Quantum", "quantum"],
			"extensions": [".qua"],
			"configuration": "./quantum-lang-config.json"
		}],
		"grammars": [{
			"language": "quantum",
			"scopeName": "source.qua",
			"path": "./quantum.tmLanguage.json"
		}],
		"snippets": [{
			"language": "quantum",
			"path": "./quantum.code-snippets"
		}]
	}
}