{
	"name": "jff",
	"displayName": "JustForFun",
	"description": "JustForFun syntax highlighting and snippets",
	"version": "0.0.1",
	"engines": { "vscode": "^1.64.0" },
	"categories": ["Programming Languages"],
	"repository": "https://github.com/ValentineBucket/vsc-jff",
	"contributes": {
		"languages": [{
			"id": "jff",
			"aliases": ["JustForFun", "jff"],
			"extensions": [".jff"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "jff",
			"scopeName": "source.jff",
			"path": "./jff.tmLanguage.json"
		}],
		"snippets": [{
			"language": "jff",
			"path": "./jff.code-snippets"
		}]
	}
}