{
	"name": "quantum",
	"author": "Val Cubeta",
	"publisher": "Val Cubeta",
	"displayName": "Quantum",
	"description": "Quantum syntax highlighting",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Programming Languages"
	],
	"repository": "https://github.com/ValCubeta/vsc-quantum",
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "quantum",
				"path": "quantum.code-snippets"
			}
		],
		"languages": [
			{
				"id": "quantum",
				"aliases": [
					"Quantum",
					"quantum"
				],
				"extensions": [
					".qua"
				],
				"configuration": "./quantum-lang-config.json"
			}
		],
		"grammars": [
			{
				"language": "quantum",
				"scopeName": "source.qua",
				"path": "./test.tmLanguage.json"
			}
		]
	}
}