class Pet {
  own priv count = 0;
  oncreate({ name, age }) {
    this.add(name, age);
    count++;
  }
  toString() => "%% { name: $(this.name), age: $(this.age) }" << this#class.name;
}

class Dog: Pet {
  woof() {
    print("$(this.name or "A dog"): Woof!");
  }
}

class Cat: Pet {
  meow() {
    print("$(this.name or "A cat"): Meow!");
  }
}

