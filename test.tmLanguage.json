{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Quantum",
	"scopeName": "source.qua",
	"patterns": [
		{
			"include": "#main"
		}
	],
	"repository": {
		"main": {
			"patterns": [
				{
					"name": "comment.line.qua",
					"begin": "//",
					"end": "\\n"
				},
				{
					"name": "comment.block.qua",
					"begin": "/\\*",
					"end": "\\*/"
				},
				{
					"name": "string.quoted.qua",
					"begin": "'|\"",
					"patterns": [
						{
							"name": "variable.qua",
							"match": "(\\$)([a-z_A-Z]+(?:\\.[a-z_A-Z]+)*)",
							"captures": {
								"1": {
									"name": "punctuation.definition.template-expression.qua"
								},
								"2": {
									"patterns": [
										{
											"include": "#main"
										}
									]
								}
							}
						},
						{
							"name": "constant.other.character-class.qua",
							"match": "\\\\[trn]"
						},
						{
							"name": "constant.character.escape.qua",
							"match": "\\\\."
						},
						{
							"contentName": "variable.qua",
							"begin": "\\$\\(",
							"beginCaptures": {
								"0": {
									"name": "punctuation.definition.template-expression.begin.qua"
								}
							},
							"patterns": [
								{
									"include": "#main"
								}
							],
							"end": "\\)",
							"endCaptures": {
								"0": {
									"name": "punctuation.definition.template-expression.end.qua"
								}
							}
						}
					],
					"end": "\\0|\\n"
				},
				{
					"name": "string.regexp.qua regexp.qua",
					"begin": "(?<=(?:^|if|unless|or|when|is|return|throw|not|and|or|in|of|while|until|[^a-z_A-Z\\s])\\s*)/(?![)?*+]|[^/]*\\n)",
					"comment": "Matches if is preceeded by a keyword or an operator and if is not followed by `)`, `?`, `*`, or `+` and there is the end",
					"patterns": [
						{
							"name": "invalid.illegal.qua",
							"match": "\\[\\]|\\{,?\\}|\\\\0"
						},
						{
							"match": "\\^|\\$",
							"name": "keyword.control.anchor.regexp"
						},
						{
							"match": "(?<=\\()(?:\\?:|\\?<?[=!]|\\?i:)",
							"name": "entity.name.qua"
						},
						{
							"match": "\\\\[1-9]",
							"name": "keyword.other.back-reference.regexp"
						},
						{
							"match": "[?+*]|\\{(?:[0-9]+|[0-9]*,[0-9]*)\\}",
							"name": "keyword.operator.quantifier.regexp"
						},
						{
							"match": "\\|",
							"name": "keyword.operator.or.regexp"
						},
						{
							"name": "constant.other.character-class.regexp.qua",
							"match": "\\\\[trnWwDdSs]"
						},
						{
							"name": "constant.character.escape.qua",
							"match": "\\\\."
						},
						{
							"name": "punctuation.definition.character-class.regexp.qua constant.other.character-class.set.regexp.qua",
							"begin": "\\[(?:(\\^)|(-))?",
							"beginCaptures": {
								"1": {
									"name": "keyword.operator.negation.regexp.qua"
								},
								"2": {
									"name": "punctuation.definition.character-class.regexp.qua constant.other.character-class.set.regexp.qua"
								}
							},
							"patterns": [
								{
									"name": "keyword.operator.qua",
									"match": "-"
								}
							],
							"end": "(-)?\\]|\\n",
							"endCaptures": {
								"1": {
									"name": "punctuation.definition.character-class.regexp.qua constant.other.character-class.set.regexp.qua"
								}
							}
						}
					],
					"end": "/(i|g|ig|gi)(?![a-z_A-Z0-9])|/",
					"endCaptures": {
						"1": {
							"name": "entity.name.qua"
						}
					}
				},
				{
					"name": "constant.numeric.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:[0-9]+(?:'[0-9]+)*(?:\\.[0-9]+)?(?:[eE][-+]?[0-9]+)?|nanum|infy|0b[01]+|0x[0-9a-fA-F]+)(?![a-z_A-Z0-9])",
					"comment": "1234567890 | 1'234'567'890 | 12345.67890 | 12345e+67890 | nanum | infy | 0b101010 | 0x5050FF"
				},
				{
					"name": "keyword.operator.type.annotation.qua meta.type.annotation.qua",
					"match": ":"
				},
				{
					"name": "keyword.operator.rest.qua",
					"match": "\\.{3}"
				},
				{
					"name": "keyword.operator.qua",
					"match": "[-+/*|]|[=!<>]=?|\\?",
					"comment": "- | + | / | * | '|' | == | != | >= | <= | ! | > | < | ?"
				},
				{
					"name": "punctuation.accessor.qua keyword.operator.accessor.qua",
					"match": "\\."
				},
				{
					"name": "keyword.operator.assignment.qua",
					"match": "[+-]?=",
					"comment": "+= | -= | ="
				},
				{
					"begin": "\\(",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.parameters.begin.qua"
						}
					},
					"patterns": [
						{
							"name": "variable.parameter.qua",
							"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])[a-z_A-Z]+(?=\\s*=)"
						},
						{
							"include": "#main"
						}
					],
					"end": "\\)",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.parameters.end.qua"
						}
					}
				},
				{
					"begin": "(fun)(?:\\s+(?:(__init__|to_string|__incl__)|([a-z_A-Z]+)))?\\s*(\\()",
					"beginCaptures": {
						"1": {
							"name": "storage.type.qua"
						},
						"2": {
							"name": "support.function.builtin.qua"
						},
						"3": {
							"name": "entity.name.function.qua"
						},
						"4": {
							"name": "punctuation.definition.parameters.begin.qua"
						}
					},
					"patterns": [
						{
							"name": "variable.parameter.qua",
							"match": "(?<![:=a-z_A-Z0-9]\\s*)[a-z_A-Z]+(?=\\s*[^a-z_A-Z0-9])"
						},
						{
							"include": "#main"
						}
					],
					"end": "\\)",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.parameters.end.qua"
						}
					}
				},
				{
					"match": "(@)\\s*([a-z_A-Z]+)",
					"captures": {
						"1": {
							"name": "keyword.operator.qua"
						},
						"2": {
							"name": "entity.name.function.decorator"
						}
					}
				},
				{
					"name": "keyword.other.import.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:import|from|as|export)(?![a-z_A-Z0-9])"
				},
				{
					"name": "keyword.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:if|unless|or|when|is|else|try|catch|then|return|throw|extends|not|and|or|for|in|of|skip|while|until|next|break|loop|del)(?![a-z_A-Z0-9])"
				},
				{
					"name": "storage.type.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:type|const|class|static)(?![a-z_A-Z0-9])"
				},
				{
					"name": "support.function.builtin.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:print|typeof|eval|isempty|(?:Syntax|Type|Range|Reference|Internal)?Error|__init__|to_string|__incl__)(?![a-z_A-Z0-9])"
				},
				{
					"name": "support.function.builtin.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:bool|num|str|obj|arr|regexp)(?![a-z_A-Z0-9])"
				},
				{
					"name": "support.class.builtin.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:any|void|Function)(?![a-z_A-Z0-9])"
				},
				{
					"name": "entity.name.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9]|(?<!\\.)\\.\\s*)(global|this|it)(?![a-z_A-Z0-9])"
				},
				{
					"name": "constant.language.qua",
					"match": "(?<!(?<!\\.)\\.\\s*|[a-z_A-Z0-9])(?:true|false|null)(?![a-z_A-Z0-9])"
				},
				{
					"name": "entity.name.function.qua",
					"match": "(?<![a-z_A-Z0-9])[a-z_A-Z]+(?=\\s*(?:\\?\\.\\s*)?(?:\\(|!(?!=)))",
					"comment": "abcdefg() | abcdefg! | NOT abcdefg !="
				}
			]
		}
	}
}